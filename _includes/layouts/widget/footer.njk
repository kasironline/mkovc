<footer class="row p-3 black">
<div class="col-md-5 p-3 p-md-5 mt-5">
<h5 class="display-4"><a href="{{metadata.url}}" class="text-white">{{ metadata.title }}</a></h5>
<small>{% set postslist = collections.posts%}
{% set postslistCounter = collections.posts | length %}
{% include "postslist.njk" %}</small>
</div>
<div class="col-md-7 p-3 p-md-5">
<h5>Hashtag</h5>
<div class="row">
{% for tag in collections.tagList | filterTagList %}
  {% set tagUrl %}/tags/{{ tag | slug }}/{% endset %}
<div class="col-md-3 col-4"><small><a href="{{ tagUrl | url }}" class="text-white tags">#{{ tag }}</a></small></div>
{% endfor %}
</div>
</div>
<div class="col-md-12 p-3 p-md-5">
<div class="row">
<h5 class="col-md-3">Menu</h5>
<div class="col-md-9">
<p class="row">
{%- for entry in collections.all | eleventyNavigation %}
<a class="text-white col-md-1 col-3" href="{{ entry.url | url }}"><small>{{ entry.title }}</small></a><br/>
{%- endfor %}
</p>
</div>
</div>
</div>
</footer>
<div class="row text-center black">
<p class="tags">copyrights <a href="{{metadata.url}}" class="text-white">{{metadata.title}}</a> <br/>
