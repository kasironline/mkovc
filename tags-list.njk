---
permalink: /tags/
layout: layouts/home.njk
---
<div class="container">
<div class="row">
<div class="col-md-12 p-3 p-md-5">
<div class="row mb-5">
{% for tag in collections.tagList | filterTagList %}
  {% set tagUrl %}/tags/{{ tag | slug }}/{% endset %}
  <h3 class="col-md-4"><a href="{{ tagUrl | url }}" class="page-list">{{ tag }}</a></h3>
{% endfor %}
</div>
</div>
</div>
</div>
